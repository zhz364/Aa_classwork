<h1>Listing all the cats!!!</h1>

<ul>
    <% @cats.each do |cat| %>
        <li> <a href="<%= cat_url(cat) %>"> <%= cat.name %> </a> </li>
        <li> <%= cat.color %> </li>
        <li> <%= cat.sex %> </li>
        <li> <%= cat.birthdate %> </li>
        <li> <%= cat.description %> </li>
        <br>
    <% end %>
</ul>